{
  "name": "phonomit",
  "version": "0.0.1",
  "description": "On-Premise P2P Audio Broadcasting",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open",
    "dev": "npm run dev-server",
    "dev-task": "vite",
    "deploy": " npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
    "build": "tsc && vite build",
    "start": "npm run assets && npm run dev",
    "start-remote": "vite --host",
    "assets": "cp -r 'node_modules/@shoelace-style/shoelace/dist/assets/icons' 'public/assets/shoelace/assets'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pwabuilder/pwainstall": "^1.6.7",
    "@shoelace-style/shoelace": "^2.0.0-beta.87",
    "@vaadin/router": "^1.7.4",
    "ably": "^1.2.34",
    "lit": "^2.3.1",
    "lit-element-state": "^1.7.0",
    "simple-peer": "^9.11.1",
    "workbox-build": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.2"
  },
  "devDependencies": {
    "@types/simple-peer": "^9.11.5",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vite-plugin-pwa": "^0.11.13"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
